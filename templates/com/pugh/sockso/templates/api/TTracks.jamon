
<%import>
    com.pugh.sockso.music.Track;
    java.util.Vector;
</%import>

<%args>
    Vector<Track> tracks = null;
</%args>

<%java>
    boolean isFirst = true;
</%java>

[

<%for final Track track : tracks %>
    <%if !isFirst%>,</%if>
    <%if isFirst%><%java>isFirst = false;</%java></%if>
    {
        "id": "<% track.getId() %>",
        "name": "<% track.getName() %>"
    }
</%for>

]